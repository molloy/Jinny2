<h1>Editing address_type</h1>

<%= form_for(@address_type) do |f| %>
	<%= render "shared/error_messages", :target => @address_type %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @address_type %> |
<%= link_to 'Back', address_types_path %>