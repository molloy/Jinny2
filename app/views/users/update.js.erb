<% if @user.errors.any? %>
	$('#j-center').html('<%= escape_javascript( render(:partial => "form") ) %>');
<% else %>
	$('#j-center').html('<%= escape_javascript( render(:partial => "index") ) %>');

	$('#search_result_panel').html('<%= escape_javascript( render(:partial => "search_result") ) %>');
	$('#search_result_header').toggle($('#search_result_panel').children().length > 0);
	$('#search_result_panel').toggle($('#search_result_panel').children().length > 0);
	
	$('#j-loading').hide();
<% end %>