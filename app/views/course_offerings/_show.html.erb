<%= semantic_form_for @course_offering do |f| %>
	<% f.inputs do %>
		<%= f.input :course, :as => 'string' %>
		<%= f.input :term_type %>
		<%= f.input :year, :as => 'string' %>
	<% end %>
<% end %>
<br />
<% if is_administrator %>
<%= link_to 'Edit', edit_course_offering_path(@course_offering) %> |
<% end %>

<% if is_student %>
<%= link_to 'Register for this class'%> |
<% end %>

<%= link_to 'Back', course_offerings_path(:search_condition_params => @search_condition_params), :remote => true %>