<div id="search_result_container_panel" class="ui-accordion ui-widget ui-helper-reset ui-accordion-icons">
	<div id="search_result_loading" class="j-loading-small" style="display: none"></div>
	<h3 id="search_result_header" class="header ui-accordion-header ui-helper-reset ui-state-default ui-corner-top">
		<span class="ui-icon ui-icon-triangle-1-s"></span>
		<a href="#">Search Results</a>
	</h3>
	<div id="search_result_panel" class="ui-accordion-content ui-helper-reset ui-widget-content ui-corner-bottom" style="display: block; overflow-x: auto; overflow-y: auto; padding-top: 11px; padding-bottom: 11px;">
		<% unless @course_takens == nil %>

			<%= button_to 'Export', { :action => 'export', :search => params[:search] } %>
			<br/>

			<%= will_paginate @course_takens %>

			<table class="search_result">
			  <tr>
			    <th>Last Name</th>
			    <th>First Name</th>
			    <th>SSN</th>
			    <th>Degree Program</th>
				<th>Course</th>
				<th>Year</th>
				<th>Term</th>
				<th>Status</th>
				<th>Grade</th>
			  </tr>
	
			<% @course_takens.each do |course_taken| %>
			  <tr>
			    <td><%=course_taken.person.surname %></td>
			    <td><%=course_taken.person.given_name %></td>
			    <td><%=course_taken.person.ssn %></td>
			    <td><%=course_taken.person.degree_program.title %></td>
			    <td><%=course_taken.course_offering.course.course_number_and_name %></td>
			    <td><%=course_taken.course_offering.year %></td>
			    <td><%=course_taken.course_offering.term_type.name %></td>
			    <td><%=course_taken.course_taken_status.name %></td>
			    <td><%=course_taken.grade_type.name %></td>
			    <!-- <td><%= link_to 'Edit', edit_course_taken_path(course_taken, :search => params[:search]), :remote => true %></td> -->
				<!-- <td><%= link_to 'Destroy', course_taken_path(course_taken, :search => params[:search]), :confirm => 'Are you sure?', :method => :delete, :remote => true %></td> -->
			  </tr>
			<% end %>
			</table>
	
			<%= will_paginate @course_takens %>
		<% end %>
	</div>
</div>
